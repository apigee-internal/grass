<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ServiceCallout enabled="true" continueOnError="true" async="false" name="GenerateAuthCode.Callout">
    <DisplayName>GenerateAuthCode.Callout</DisplayName>
    <FaultRules/>
    <Properties/>
    <Request variable="authcode.request" clearPayload="false">
        <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
    </Request>
    <Response>authcode.response</Response>
    <HTTPTargetConnection>
        <Properties/>
        <URL>http://op102-ex-test.apigee.net/oauth/v2/code?response_type=code&amp;client_id={client_id}&amp;scope={scope}&amp;redirect_uri={redirect_uri}&amp;userid={userid}&amp;sessionid={sessionid}</URL>
    </HTTPTargetConnection>
</ServiceCallout>